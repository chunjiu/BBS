<%-include('header')%>


  <div class="row">
    <div class="col-md-2 col-md-push-10">
      <%if(current_user){%>
        <div class="panel panel-default panelBorderNone">
          <div class="panel-heading">个人信息</div>
          <%-include('sidebar')%>
        </div>
        <div>
          <a href="/create">
             <span class="btn btn-success">发布话题</span>
          </a>
        </div>
       <%}else{%>
        <%-include('sidebar2')%>
       <%}%>
    </div>
    <div class='col-md-10 col-md-pull-2'>
      <div class="panel noborder topcinav" style="border-style:none">
        <div class="panel-heading">
          <ul class="nav nav-pills">
            <li><a href="#">全部</a></li>
            <li><a href="#">精华</a></li>
            <li><a href="#">分享</a></li>
            <li><a href="#">问答</a></li>
            <li><a href="#">招聘</a></li>
            <li><a href="#">客户端测试</a></li>
          </ul>
        </div>
        <div style="background-color:white">
          <table class=" table table-hover" style="border-style:none">
            <%if(topic.length){%>
              <%topic.forEach(function(topic){%>
                <tr>
                  <td>
                    <span title="回复数"><%=topic.reply_count%></span>/<span title="点击数"><%=topic.visit_count%></span>
                    <span class="TopicTab"><%=topic.tab%></span>
                    <a style="text-decoration:none;" href="/<%=topic.id%>/tid" title="<%=topic.title%>">
                      <%=topic.title%>
                    </a>
                    <span class="pull-right"><%=topic.create_at_ago()%><span>
                  </td>
                </tr>
              <% })%>
            <%}else{%>
                <div class="inner">
                  <p>无话题</p>
                </div>
            <%}%>
          </tabel>
        </div>
      </div>
    </div>
    <!--col-sm-8-->
  </div>
  <!--row-->



  <%-include('footer')%>
